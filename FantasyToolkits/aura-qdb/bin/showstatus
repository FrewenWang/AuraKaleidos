#!/usr/bin/expect -f
spawn adb root
spawn adb remount
spawn adb shell

expect "e171" { send "busybox telnet 192.168.78.2; exit\r" }
expect "login" { send "root\r" }
expect "#" { send "date\r" }
expect "#" { send "pidin arg | grep pavaro\r" }
expect "#" { send "hogs -i 1 | grep pavaro\r" }
expect "#" { send "aps\r" }
expect "#" { send "exit\r" }

interact
