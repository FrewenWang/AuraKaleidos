#!/bin/bash
adb root
adb remount

expect << EOF
    spawn adb shell

    expect "e171" { send "busybox telnet 192.168.78.2; exit\r" }
    expect "login" { send "root\r" }
    expect "Password" { send "togxuh-8Cyzto-juqven\r" }
    expect "#" { send "sh /mnt/vendor/adas/scripts/stoprecord.sh\r" }
    expect "#" { send "exit\r" }
    expect eof
EOF

